<div class="container">

  <h1>Create new task</h1>

  <%= simple_form_for(@task) do |f| %>
    <%= f.input :title %>
    <%= f.input :details %>
    <%= f.button :submit, class: "btn btn-primary" %>

  <% end %>

  <%= link_to "Back to tasks", tasks_path, class: "btn btn-primary link"  %>

</div>
  